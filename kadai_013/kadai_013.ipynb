      "source": [
        "# 商品価格と消費税率から税込金額を計算して返す関数\n",
        "def tax(price: int, tax_rate: int) -> float:\n",
        "\n",
        "    # 消費税額（小数点を保持）\n",
        "    tax_amount = price * (tax_rate / 100)\n",
        "\n",
        "    # 税込価格\n",
        "    total_price = price + tax_amount\n",
        "\n",
        "    # 数値のみで出力\n",
        "    return total_price\n",
        "\n",
        "\n",
        "# 引数：110円 :10%\n",
        "result = tax(110, 10)\n",
        "print(result)\n"
      ],
