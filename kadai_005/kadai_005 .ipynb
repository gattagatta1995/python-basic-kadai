        "lower_part = 20\n",
        "\n",
        "# 高さの数値\n",
        "height = 5\n",
        "\n",
        "# 台形の面積 = (上辺 + 下辺) × 高さ ÷ 2\n",
        "area = (upper_part + lower_part) * height / 2\n",
        "\n",
        "print(f\"{area}cm²\")"
